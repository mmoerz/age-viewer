{
  "name": "Dev Apache Age Viewer",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "apache-age-viewer",
  "workspaceFolder": "/src",
  "context": "..",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"     
      },
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "ms-vscode.vscode-typescript-next",
        "xabikos.javascriptsnippets"
      ]
    }
  },
  "postCreateCommand": "/src/.devcontainer/apache-age-viewer/setup.sh",
  "remoteUser": "node"
}